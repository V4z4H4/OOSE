Class Date{
  int day;
  int month;
  int year;
   
  Date(int d,int m,int y){
    this.day = d;
    this.month = m;
    this.year = y;
  }
  public String toString(){
     
    return day+"."+month+"."+year;
   
  }
  public static void main(String[] args) {
     
    Date d1 = new Date(12,04,1993);
     
    System.out.println(d1.toString());
     
  }    
}
